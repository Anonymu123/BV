  (function () {
    const accessibility = () => { 
      console.log('accessibility');  
      const clickEvent = {
        event: 'button_click',
        component: 'widget',
        title: '-',
        text: 'acessibilidade',
      };
      window.gaDatalayer.sendCtaClickEvent(clickEvent);
    };
    const libra = () => {   
      console.log('libra'); 
      const clickEvent = {
        event: 'button_click',
        component: 'widget',
        title: '-',
        text: 'acessibilidade-libras',
      };
      window.gaDatalayer.sendCtaClickEvent(clickEvent);
    };
    const clickEvents = () => {
      document.querySelector('#INDmenu-btn').addEventListener('click', accessibility);
      document.querySelector('div.sc-cmTdod.clHDGZ > button').addEventListener('click', libra);
    };
    const startGa = () => {
      clickEvents();
    };
    const gaDatalayerLoaded = () => {
      const CHECK_INTERVAL = 100;
      const checkGaDatalayerLoaded = window.setInterval(() => {
        const isLoaded = window.gaDatalayer;
        const btnEqualWeb = document.querySelector('#INDmenu-btn');
        const btnHandTalk = document.querySelector('div.sc-cmTdod.clHDGZ > button');
        if (isLoaded && btnEqualWeb && btnHandTalk) {
          window.clearInterval(checkGaDatalayerLoaded);
          startGa();
        }
      }, CHECK_INTERVAL);
    };
    document.addEventListener('DOMContentLoaded', gaDatalayerLoaded);
  })();

</script>             

