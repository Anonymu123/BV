    <script>
    const setGaEventsOnFooter = (listSelector) => {
      const footerSelector = document.querySelector(".main-footer");
  
      const footerGroup = footerSelector.querySelector(listSelector);
  
      let dataSetList = footerGroup.dataset.footer;
  
      const fortmatText = (text) => {
        text = text.toLowerCase();
        text = text.replace(new RegExp("[ÁÀÂÃ]", "gi"), "a");
        text = text.replace(new RegExp("[ÉÈÊ]", "gi"), "e");
        text = text.replace(new RegExp("[ÍÌÎ]", "gi"), "i");
        text = text.replace(new RegExp("[ÓÒÔÕ]", "gi"), "o");
        text = text.replace(new RegExp("[ÚÙÛ]", "gi"), "u");
        text = text.replace(new RegExp("[Ç]", "gi"), "c");
        text = text.replace(new RegExp("/<br\s*[\/]?>/", "gi"), "-");
        text = text.replace(new RegExp("[.,;!?]", "g"), "");
        text = text.trim();
        text = text.replaceAll(/ /g, "-");
        return text;
      };
  
      let listLinks = footerGroup.querySelectorAll("a");
  
      const setClickEventsOnLinks = () => {
        listLinks.forEach((linkListItem) => {
          const CHECK_INTERVAL = 100;
  
          const checkTextOnLink = window.setInterval(() => {
  
            let formatedTextLink = fortmatText(linkListItem.textContent);
            let eventTag = "link_click";
  
            if(dataSetList === "siga-o-bv" || dataSetList === "baixe-o-nosso-app" ) {
              let titleApp = linkListItem.getAttribute("title");
              formatedTextLink = fortmatText(titleApp);
              eventTag = "button_click";
            }
  
            if (formatedTextLink.length > 0) {
              window.clearInterval(checkTextOnLink);
  
              let titleTag = "";
              if (listSelector !== "[data-footer='other-links']") {
                titleTag = dataSetList;
              }
  
              linkListItem.addEventListener("click", () => {
                let clickEvent = {
                  event: eventTag,
                  component: "footer",
                  title: titleTag,
                  text: formatedTextLink,
                };
                window.gaDatalayer.sendCtaClickEvent(clickEvent);
              });
            }
          }, CHECK_INTERVAL);
        });
      };
  
      const gaDatalayerLoaded = () => {
        const CHECK_INTERVAL = 100;
  
        const checkGaDatalayerLoaded = window.setInterval(() => {
          const isLoaded = window.gaDatalayer;
  
          if (isLoaded) {
            window.clearInterval(checkGaDatalayerLoaded);
            setClickEventsOnLinks();
          }
        }, CHECK_INTERVAL);
      };
  
      gaDatalayerLoaded();
    };
  
    document.addEventListener("DOMContentLoaded", () => {
      setGaEventsOnFooter("[data-footer='sobre-o-bv']");
      setGaEventsOnFooter("[data-footer='produtos']");
      setGaEventsOnFooter("[data-footer='atendimento']");
      setGaEventsOnFooter("[data-footer='conheca-tambem']");
      setGaEventsOnFooter("[data-footer='baixe-o-nosso-app']");
      setGaEventsOnFooter("[data-footer='other-links']");
      setGaEventsOnFooter("[data-footer='siga-o-bv']");
    });
  </script>
  
  
    
