<script>
  document.BVloadJS = function(url, attrs, cb, location){
    var scriptTag = document.createElement('script');
    scriptTag.src = url;

    if (attrs) {
      for (var i = 0; i < attrs.length; i++) {
        var attrObj = attrs[i];

        for (var attrKey in attrObj) {
          if (Object.hasOwnProperty.call(attrObj, attrKey)) {
            var attrValue = attrObj[attrKey];
            scriptTag.setAttribute(attrKey, attrValue);
          }
        }
      }
    }
  
    if (cb) {
      scriptTag.onload = cb;
      scriptTag.onreadystatechange = cb;
    }
  
    location.appendChild(scriptTag);
  };
  
  document.BVloopExternalScripts = function() {
  	for (var index = 0; index < document.BVexternalScripts.length; index++) {
		var objScript = document.BVexternalScripts[index];
		var path = objScript.path;
		var callback = objScript.callback;
		var attributes = objScript.attributes;
	
		document.BVloadJS(path, attributes, callback, document.body);
	}
	
    var elContent = document.querySelector('#content');
    var elSectionHero = document.querySelector('.section-hero.-background-image');
    var elBreadcrumbs = document.querySelector('.bv-breadcrumbs');
	
    if (elContent) {
      elContent.style.marginTop = '76px';
    }
    if (elSectionHero) {
      elSectionHero.style.marginTop = '76px';
    }
    if (elBreadcrumbs) {
      elBreadcrumbs.style.top = '76px';
    }
  }
  
  if (document.readyState === "loading") {
    document.addEventListener("DOMContentLoaded", document.BVloopExternalScripts);
  } else {
    document.BVloopExternalScripts();
  }
</script>


