  <script>
    function initChatButtons() {
      var $ = jQuery;
      $('.chat-widget__btn').on('click', function (event) {
        event.preventDefault();
        document.dispatchEvent(new CustomEvent('openModal'));

        triggerDataLayer(
          'chatbot-flutuante|click-chatbot',
          'chatbot_click'
        );
      });
    }

    function triggerDataLayer(internalCampaign, eventName) {
      if (document.BVDatalayer === undefined) {
        document.BVDatalayer = {};
      }

      document.BVDatalayer.internalCampaign = internalCampaign;
      document.dispatchEvent(new CustomEvent(eventName));
    }

    function bootstrapChatbot() {
      var isChatReady;
      var _fila;

      document.addEventListener('openChatBotSalesforce', openChatBotSalesForce);

      document.addEventListener('fecharChatBot', fecharChatBot);

      function fecharChatBot() {
        triggerDataLayer(
          'chatbot-flutuante|fechar-chat|click',
          'chatbot_click_fechar'
        );
      }

      function triggerDataLayer(internalCampaign, eventName) {
        if (document.BVDatalayer === undefined) {
          document.BVDatalayer = {};
        }

        document.BVDatalayer.internalCampaign = internalCampaign;
        document.dispatchEvent(new CustomEvent(eventName));
      }

      function addEventOnFrameReady() {
        if (embedded_svc.isIframeReady) {
          setTimeout(() => document.dispatchEvent(new CustomEvent('closeModal')), 8000);

          var sidebarBody = document.querySelector('.dialog-button-1');

          if (sidebarBody) {

            sidebarBody.addEventListener('click', function () {
              document.dispatchEvent(new CustomEvent('openModalChat'));
            });

            clearInterval(isChatReady);
          }
        }

      }

      function openChatBotSalesForce(e) {
        if (e.detail) {
          var _details = [];
          var params = e.detail;

          for (p in params) {
            var _obj = {};
            _obj.label = p;
            _obj.value = params[p];
            _obj.transcriptFields = [p + '__c'];
            _obj.displayToAgent = false;

            _details.push(_obj);
          }

          initESW('https://service.force.com/', _details);
          isChatReady = setInterval(addEventOnFrameReady, 1000);
        }
      }

      // Salesforce chat
      function initESW(gslbBaseURL, details) {
        embedded_svc.settings.displayHelpButton = false; //Or false
        embedded_svc.settings.language = 'pt-BR'; //For example, enter 'en' or 'en-US'
        embedded_svc.settings.enabledFeatures = ['LiveAgent'];
        embedded_svc.settings.entryFeature = 'LiveAgent';

        if (details && details.length > 0) {
          details.forEach(function (arr, i) {
            if (arr.label === 'Fila') {
              _fila = details[i].value;
              delete details[i];
            }
          });
          embedded_svc.settings.extraPrechatFormDetails = details;
        }

        embedded_svc.init(
          _fila.url,
          _fila.liveAgentSetupFlow + '/liveAgentSetupFlow',
          gslbBaseURL,
          _fila.organizationId,
          'Chat_BV', {
            baseLiveAgentContentURL: _fila.baseLiveAgentContentURL + '/content',
            deploymentId: _fila.deploymentId,
            buttonId: _fila.buttonId,
            baseLiveAgentURL: _fila.baseLiveAgentURL + '/chat',
            eswLiveAgentDevName: 'Chat_BV',
            isOfflineSupportEnabled: false
          }
        );

        setTimeout(() => embedded_svc.bootstrapEmbeddedService(), 1500);
      }
    }

    function loadResources(resources, callback) {
      var loaded = 0;

      function onResorceLoad() {
        loaded += 1;

        // if all resources loaded, we are finished
        if (loaded >= resources.length) {
          callback();
        }
      }

      resources.map(function (resource) {
        if (resource.type === 'stylesheet') {
          var link = document.createElement('link');
          link.rel = 'stylesheet';
          link.href = resource.src;
          link.onload = onResorceLoad;
          document.head.appendChild(link);
        }
        else if (resource.type === 'script') {
          var script = document.createElement('script');
          script.src = resource.src;
          script.onload = onResorceLoad;
          document.head.appendChild(script);
        }
        else {
          throw new Error('invalid resource type');
        }
      });
    }

    function handleOpenChatbotEvent() {
      // remove o event listener dessa função para não executar mais de uma vez
      document.removeEventListener('openModal', handleOpenChatbotEvent);

      // mostra o spinner no lugar do botão de chat
      var chatWidget = document.querySelector('.chat-widget');
      chatWidget.classList.add('-loading');

      // cria o elemento raiz onde será inserido o app do chatbot
      var app = document.createElement('app-foundation-interface-chatbot');
      document.body.appendChild(app);

      // cria uma lista com os recursos que devem ser carregados antes de tudo
      var preResources = [
        { type: 'script', src: '/site/system/modules/opencmslib.bvbt.base.foundation.interface.chatbot/resources/bvbt/foundation-interface-chatbot-vendor.js?1664906290847' },
      ];

      // só inclui o zone.js se já não tiver sido carregado anteriormente na página
      if (!window['Zone']) {
        preResources.push({ type: 'script', src: '/site/system/modules/opencmslib.bvbt.base.foundation.interface.chatbot/resources/bvbt/zone.js?1664906290939' });
      }

      // lista com os recursos necessários do chatbot
      var mainResources = [
        { type: 'stylesheet', src: '/export/system/modules/opencmslib.bvbt.base.foundation.interface.chatbot/resources/bvbt/styles.css_505658261.css' },
        { type: 'script', src: '/site/system/modules/opencmslib.bvbt.base.foundation.interface.chatbot/resources/bvbt/foundation-interface-chatbot.js?1664906290783' },
        { type: 'script', src: 'https://bancobv--uat.my.salesforce.com/embeddedservice/5.0/esw.min.js' }
      ];

      // carrega primeiro os "preResources", depois os "mainResources"
      loadResources(preResources, function () {
        loadResources(mainResources, function () {
          // inicializa o chatbot
          bootstrapChatbot();

          // remove o spinner do chat
          chatWidget.classList.remove('-loading')

          // dispara novamente o evento, que agora vai ser capturado pela própria aplicação do chatbot
          document.dispatchEvent(new CustomEvent('openModal'));
        });
      });
    }

    document.addEventListener('openModal', handleOpenChatbotEvent);

    if (document.readyState !== 'loading') {
      initChatButtons();
    } else {
      document.addEventListener('DOMContentLoaded', function () {
        initChatButtons();
      });
    }
  </script>
</div>

  <script>delete window.webpackJsonp</script>
</div>
        
      

