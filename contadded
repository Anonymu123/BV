    <script>
    const setGaEventsOnHeader = (menuVersion, menuSelector, linkSelector) => {
      const menu = document.querySelector(menuVersion);
  
      if (menu) {
        const menuVisible = menu.querySelector(menuSelector);
        let dataSetMenu = menuVisible.dataset.menu.replace("-mobile", "");
  
        const fortmatText = (text) => {
          text = text.toLowerCase();
          text = text.replace(new RegExp("[ÁÀÂÃ]", "gi"), "a");
          text = text.replace(new RegExp("[ÉÈÊ]", "gi"), "e");
          text = text.replace(new RegExp("[ÍÌÎ]", "gi"), "i");
          text = text.replace(new RegExp("[ÓÒÔÕ]", "gi"), "o");
          text = text.replace(new RegExp("[ÚÙÛ]", "gi"), "u");
          text = text.replace(new RegExp("[Ç]", "gi"), "c");
          text = text.replace(new RegExp("/<br\s*[\/]?>/", "gi"), "-");
          text = text.replace(new RegExp("[.,;!?]", "g"), "");
          text = text.trim();
          text = text.replaceAll(/ /g, "-");
          return text;
        };
  
        let listLinks = menuVisible.querySelectorAll(linkSelector);
  
        const navItem = 'nav-item';
  
        const setClickEvents = () => {
          listLinks.forEach((linkListItem) => {
            const CHECK_INTERVAL = 100;
  
            const checkTextOnLink = window.setInterval(() => {
              let formatedTextLink = fortmatText(linkListItem.textContent);
  
              if (formatedTextLink.length > 0) {
                window.clearInterval(checkTextOnLink);
  
                let titleTag = '';
                if(menuVersion === '.ia-menu-control'){
                  titleTag = "menu";
                }
                if(menuVersion === '.ia-menu-experience'){
                  titleTag = "novo-menu-2022";
                }
  
  
                if(dataSetMenu !== navItem) {
                  linkListItem.addEventListener("click", () => {
                    let clickEvent = {
                      event: "link_click",
                      component: "menu-header",
                      title: titleTag,
                      text: dataSetMenu + ":" + formatedTextLink,
                    };
                    window.gaDatalayer.sendCtaClickEvent(clickEvent);
                  });
                }
  
                if(dataSetMenu === navItem) {
                  linkListItem.addEventListener("click", () => {
                    let clickEvent = {
                      event: "link_click",
                      component: "menu-header",
                      title: titleTag,
                      text: formatedTextLink,
                    };
                    window.gaDatalayer.sendCtaClickEvent(clickEvent);
                  });
                }
              }
            }, CHECK_INTERVAL);
          });
        };
  
        const gaDatalayerLoaded = () => {
          const CHECK_INTERVAL = 100;
  
          const checkGaDatalayerLoaded = window.setInterval(() => {
            const isLoaded = window.gaDatalayer;
  
            if (isLoaded) {
              window.clearInterval(checkGaDatalayerLoaded);
              setClickEvents();
            }
          }, CHECK_INTERVAL);
        };
  
        gaDatalayerLoaded();
      }
    };
  
