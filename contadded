  <script>
    document.addEventListener('DOMContentLoaded', function(event) {
      var ls =  window.localStorage;
      var hasAcceptedCookies = ls.getItem('acceptCookies');

      if (!hasAcceptedCookies) {
        var cookiesAlert = document.querySelector('.js-cookies-alert');
        var acceptBtn = document.querySelector('.js-accept-cookies');
        cookiesAlert.classList.add('is-visible');
        acceptBtn.addEventListener('click', function() {
          ls.setItem('acceptCookies', '1');
          cookiesAlert.classList.remove('is-visible');
        }, false);
      }
    });
  </script>

