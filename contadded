  <script>
document.BVexternalScripts.push({
  path: "//assets.adobedtm.com/865088ed6c02/4db5ae3c60b7/launch-bec202e9eb41.min.js",
  attributes: [],
  callback: function() {},
});
</script>
  








<script>
  (function () {
    function getParameter(name, url) {
      var params = url.replace(/^[#?]/, "").split("&");
      for (var i = 0; i < params.length; i++) {
        var parts = params[i].split("=", 2);
        if (parts[0] === name) {
          return parts.length > 1 ? parts[1] : "";
        }
      }
      return undefined;
    }

    function merge(dest, src) {
      for (var prop in src) {
        if (src.hasOwnProperty(prop)) {
          var isObj =
            typeof dest[prop] === "object" && typeof src[prop] === "object";
          var isArray =
            dest[prop] instanceof Array || src[prop] instanceof Array;

          if (isObj && !isArray) {
            merge(dest[prop], src[prop]);
          } else {
            dest[prop] = src[prop];
          }
        }
      }
      return dest;
    }

    function BVCustomPageViewEvent(params) {
      params = params || { bubbles: false, cancelable: false, detail: null };
      var evt = document.createEvent("CustomEvent");
      evt.initCustomEvent(
        "BVCustomPageView",
        params.bubbles,
        params.cancelable,
        params.detail
      );
      return evt;
    }

    // Initialize datalayer structure with default values
    document.BVDatalayer = document.BVDatalayer || {};
    document.BVDatalayer.pageInfo = document.BVDatalayer.pageInfo || {};
    document.BVDatalayer.pageInfo.campaign =
      document.BVDatalayer.pageInfo.campaign || {};
    document.BVDatalayer.pageInfo.URL =
      document.BVDatalayer.pageInfo.URL || document.URL;
    document.BVDatalayer.pageInfo.pageName =
      document.BVDatalayer.pageInfo.pageName || "bv|aa|conta";
    document.BVDatalayer.pageInfo.siteSection =
      document.BVDatalayer.pageInfo.siteSection || "aberta";

    // Inserted campaign parameters from URL (ef_id) into the datalayer
    if (!document.BVDatalayer.pageInfo.campaign.AMOId) {
      var AMOId =
        getParameter("ef_id", document.location.search) ||
        getParameter("ef_id", document.location.hash);

      if (AMOId) {
        document.BVDatalayer.pageInfo.campaign.AMOId = AMOId;
      }
    }

    // Export utility functions
    var dl = {};
    dl._autoPageView = true;

    dl.get = function get() {
      return document.BVDatalayer;
    };

    dl.update = function update(data) {
      merge(document.BVDatalayer, data);
      return dl;
    };

    dl.pageView = function pageView() {
      document.dispatchEvent(new BVCustomPageViewEvent());
      return dl;
    };

    dl.disableAutoPageView = function () {
      dl._autoPageView = false;
    }

    dl.enableAutoPageView = function () {
      dl._autoPageView = true;
    }

    dl.trigger = function () {
      if (dl._autoPageView) {
        dl.pageView();
      }
    }

    window._datalayer = dl;
  })();
</script>

  

  
    
